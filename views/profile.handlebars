<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Profile</title>
  <link rel="stylesheet" href="styles.css">
</head>

<body>
  {{#robots}}
  <section class="profile">
    <img class="robotsImg" src="{{avatar}}" alt="">
    <section class="info">
      <h1>{{name}}</h1>
      <p>{{address.city}}, {{address.country}} | {{job}} at {{company}}</p>
      <p>{{email}} | {{phone}}</p>
    </section>
  </section>
  <table>
    <tr>
      <td>
        <h4>Education</h4></td>
      <td>{{university}}</td>
    </tr>
    <tr>
      <td>
        <h4>Skills</h4></td>
      <td>
        <ul>
          {{#skills}}
          <li>{{.}}</li>
          {{/skills}}

        </ul>

      </td>

    </tr>
  </table>

{{/robots}}


</body>

</html>
